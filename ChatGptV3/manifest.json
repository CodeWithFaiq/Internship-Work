{
    "name":"ChatgptProject",
    "version":"0.0.1",
    "manifest_version":3,
    "action":{
        
        "default_icon":"download.png",
        "default_popup": "popup.html"
    },
    "content_scripts":[
        {
      "matches":["https://*.chat.openai.com/chat/*"],
      "js":["jquery.js", "popper.js", "bootstrap.js",  "content.js"],
      "css":["bootstrap.css","styles.css"]
      
        }
    ],
    "permissions": [
    "tabs",
    "bookmarks",
    "unlimitedStorage"
  ]    

}