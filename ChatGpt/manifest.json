{
    "name":"ChatgptProject",
    "version":"0.0.1",
    "manifest_version":3,
    "action":{
        
        "default_icon":"download.png"
    },
    "content_scripts":[
        {
      "matches":["https://*.chat.openai.com/chat/*"],
      "js":["jquery.js", "bootstrap.js","popper.js", "content.js"],
      "css":["bootstrap.css","styles.css"]
      
        }
    ],
    "permissions": [
    "tabs",
    "bookmarks",
    "unlimitedStorage"
  ]
    

}